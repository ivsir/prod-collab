tainer>\n      <div className=\"top-container\">\n        <Slide className=\"slide-text\">\n          <h1>What do you want to create?</h1>\n        </Slide>\n\n        <Fade className=\"fade-text\" delay={1e3} cascade damping={1e-1}>\n          .........Create a new project and it share with the community\n        </Fade>\n\n        {Auth.loggedIn() ? (\n          <>\n            <form\n              className=\"flex-row justify-center justify-space-between-md align-center box\"\n              onSubmit={handleFormSubmit}\n            >\n              <div className=\" col-lg-9 textarea-div\">\n                <textarea\n                  className=\"first-textarea\"\n                  name=\"projectTitle\"\n                  placeholder=\"Project Title...\"\n                  value={projectTitle}\n                  style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                  onChange={handleChange}\n                ></textarea>\n              </div>\n              <div className=\"col-12 col-lg-9 textarea-div\">\n                <textarea\n                  name=\"projectDescription\"\n                  placeholder=\"Project Description...\"\n                  value={projectDescription}\n                  className=\"form-input w-100\"\n                  style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                  onChange={handleChange}\n                ></textarea>\n              </div>\n\n              <div className=\"col-12 col-lg-3 button-container\">\n                <button\n                  className=\"btn btn-primary btn-block py-3\"\n                  type=\"submit\"\n                >\n                  Add Project\n                </button>\n              </div>\n              {error && (\n                <div className=\"col-12 my-3 bg-danger text-white p-3 error-message\">\n                  {error.message}\n                </div>\n              )}\n            </form>\n          </>\n        ) : (\n          <p className=\"error-message\">\n            You need to be logged in to create a project. Please{\" \"}\n            <Link className=\"error-login\" to=\"/\">\n              login\n            </Link>\n            .\n          </p>\n        )}\n      </div>\n    </FormContainer>\n  );\n};\n\nexport default ProjectForm;\n","/Users/risvitareq/code/CodeCollabNew/client/src/components/accountBox/index.js",[],[],"/Users/risvitareq/code/CodeCollabNew/client/src/components/accountBox/Common.js",[],[],"/Users/risvitareq/code/CodeCollabNew/client/src/utils/auth.js",["331"],[],"/Users/risvitareq/code/CodeCollabNew/client/src/utils/queries.js",[],[],"/Users/risvitareq/code/CodeCollabNew/client/src/utils/mutations.js",[],[],"/Users/risvitareq/code/CodeCollabNew/client/src/components/accountBox/LoginForm.js",["332","333"],[],"/Users/risvitareq/code/CodeCollabNew/client/src/components/accountBox/AccountContext.js",[],[],"/Users/risvitareq/code/CodeCollabNew/client/src/components/accountBox/SignupForm.js",["334","335","336"],[],"/Users/risvitareq/code/prod-collab/client/src/index.js",[],[],[],"/Users/risvitareq/code/prod-collab/client/src/App.js",[],[],"/Users/risvitareq/code/prod-collab/client/src/components/Nav.js",["337"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/ProfileCard.js",[],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/ExploreCard.js",["338"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/About.js",[],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/ProjectForm.js",["339"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/SingleProject.js",["340","341","342"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/Comment.js",["343","344","345","346"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/index.js",["347","348"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/Common.js",["349"],[],"/Users/risvitareq/code/prod-collab/client/src/utils/auth.js",["350"],[],"import decode from 'jwt-decode';\n\nclass AuthService {\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  loggedIn() {\n    const token = this.getToken();\n    // If there is a token and it's not expired, return `true`\n    return token && !this.isTokenExpired(token) ? true : false;\n  }\n\n  isTokenExpired(token) {\n    // Decode the token to get its expiration time that was set by the server\n    const decoded = decode(token);\n    // If the expiration time is less than the current time (in seconds), the token is expired and we return `true`\n    if (decoded.exp < Date.now() / 1000) {\n      localStorage.removeItem('id_token');\n      return true;\n    }\n    // If token hasn't passed its expiration time, return `false`\n    return false;\n  }\n\ngetUsername() {\n  const token = localStorage.getItem('id_token');\n  const decodedToken = decode(token);\n  const username = decodedToken.data.username;\n  return username;\n}\n\ngetId() {\n  const token = localStorage.getItem('id_token');\n  const decodedToken = decode(token);\n  const id = decodedToken.data._id;\n  return id;\n}\n\n  getToken() {\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/explore');\n  }\n\n  logout() {\n    localStorage.removeItem('id_token');\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();","/Users/risvitareq/code/prod-collab/client/src/utils/mutations.js",[],[],"/Users/risvitareq/code/prod-collab/client/src/utils/queries.js",[],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/SignupForm.js",["351","352","353","354"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/LoginForm.js",["355","356","357"],[],"/Users/risvitareq/code/prod-collab/client/src/components/accountBox/AccountContext.js",[],[],"/Users/risvitareq/code/prod-collab/client/src